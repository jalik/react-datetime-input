  "description": "Date and time picker for React",
  "esnext": "src/index.js",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "babel src -d dist",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist",
    "dev": "babel --watch src -d dist",
